{
  "name": "express-reaccess",
  "version": "1.0.0",
  "description": "Express/Connect middleware to manage API access on a RegExp basis",
  "main": "src/index.js",
  "scripts": {
    "test": "node_modules/mocha/bin/mocha tests/*.mocha.js",
    "coveralls": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- tests/*.mocha.js -R spec -t 5000 && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cover": "./node_modules/istanbul/lib/cli.js cover --report html ./node_modules/mocha/bin/_mocha -- tests/*.mocha.js -R spec -t 5000"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/SimpliField/express-reaccess.git"
  },
  "keywords": [
    "express",
    "connect",
    "middleware",
    "auth",
    "access",
    "rights",
    "regexp"
  ],
  "author": "Nicolas Froidure (Simplifield)",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/SimpliField/express-reaccess/issues"
  },
  "devDependencies": {
    "mocha": "~2.1.0",
    "supertest": "~0.15.0",
    "express": "~4.10.6",
    "mocha-lcov-reporter": "0.0.1",
    "coveralls": "~2.11.2",
    "istanbul": "~0.3.5"
  },
  "dependencies": {
    "miniquery": "^1.0.0",
    "regexp-tpl": "^1.0.0"
  }
}
